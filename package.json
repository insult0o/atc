{
  "name": "pdf-processing-platform",
  "version": "1.0.0",
  "description": "AI-powered PDF processing platform",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "PORT=3000 next dev",
    "dev:clean": "./scripts/start-dev.sh",
    "dev:clear-ports": "./scripts/clear-ports.sh",
    "dev:emergency-clear": "./scripts/emergency-port-clear.sh",
    "build": "next build",
    "start": "PORT=3000 next start",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "test:headed": "playwright test --headed",
    "test:report": "playwright show-report",
    "test:e2e": "playwright test",
    "test:e2e:quick": "playwright test --project=quick-extract",
    "test:e2e:detailed": "playwright test --project=detailed-review",
    "test:e2e:collab": "playwright test --project=collaborative",
    "test:e2e:batch": "playwright test --project=batch-processing",
    "test:e2e:api": "playwright test --project=api-integration",
    "test:e2e:visual": "playwright test --project=visual-regression",
    "test:e2e:ci": "playwright test --reporter=list,json,html"
  },
  "dependencies": {
    "@dqbd/tiktoken": "^1.0.21",
    "@heroicons/react": "^2.2.0",
